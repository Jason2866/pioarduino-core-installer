[MASTER]
jobs=1

[REPORTS]
output-format=colorized

[MESSAGES CONTROL]
disable=
    missing-docstring,
    ungrouped-imports,
    invalid-name,
    duplicate-code,
    fixme,
    arguments-differ,
    useless-object-inheritance,
    super-with-arguments,
    raise-missing-from,
    consider-using-f-string,
    unspecified-encoding
